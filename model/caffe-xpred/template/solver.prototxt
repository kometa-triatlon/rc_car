# The training protocol buffer definition
net: "trainval.prototxt"
test_iter: 5
test_interval: 51
base_lr: 0.001
weight_decay : 0.0005
type : "Adam"
momentum: 0.9
momentum2: 0.999
lr_policy: "step"
gamma: 0.5
stepsize: 150
display: 51
max_iter: 1000
# snapshot intermediate results
snapshot: 50
snapshot_prefix: "exp/caffe_xpred"
solver_mode: GPU
